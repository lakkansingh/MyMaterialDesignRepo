<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nutrient.lakkan.mymaterialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StandaloneToolbar"></activity>
        <activity android:name=".ToolbarAsActionBar"></activity>
        <activity android:name=".ContextualMenu"></activity>
        <activity android:name=".BuildingBlockSnackBar"></activity>
        <activity android:name=".FAB"></activity>
        <activity android:name=".AnimationMaterialDesign"></activity>
        <activity android:name=".RippleAnimationActivity"></activity>
        <activity android:name=".SharedElementActivity"></activity>
    </application>


</manifest>